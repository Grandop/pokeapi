{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": [
    "next",
    "prettier",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "import/order": [
      "error",
      {
        "groups": ["external", "internal"]
      }
    ]
  }
}